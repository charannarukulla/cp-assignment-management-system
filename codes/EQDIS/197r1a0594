#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int k;
	    cin>>k;
	   set<int> s;
	    for(int i=0;i<k;i++)
	   {
	       int tmp;
	       cin>>tmp;
	       s.insert(tmp);
	   }
	    if(s.size()!=k||s.size()%2==0)std::cout << "YES" << std::endl;
	    else
	    std::cout << "NO" << std::endl;
	}
	return 0;
}
