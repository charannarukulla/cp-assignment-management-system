  </head>

  <body>
     <!-- Layout wrapper -->
     <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
          <!-- Menu -->
    <aside id="layout-menu" class="layout-menu menu-vertical menu bg-menu-theme">
        <div class="app-brand" style="margin-top: 40px;">
          <a href="index" class="app-brand-link gap-2">
             
           <img src="./cpmlogo.png"  />
          </a>  
          <span class="app-brand-text demo text-body fw-bolder"  > CPAM</span>
        </div>

        <div class="menu-inner-shadow"></div>

        <ul class="menu-inner py-1">
          <!-- Dashboard -->
          <li class="menu-item">
            <a href="dashboard" class="menu-link">
              <i class="menu-icon tf-icons bx bx-home-circle"></i>
              <div data-i18n="Analytics">Dashboard</div>
            </a>
          </li>
         
          <li class="menu-item">
            <a href="viewstuassignments" class="menu-link">
              <i class='bx bx-show'></i>
              <div data-i18n="Analytics">View assignments</div>
            </a>
          </li>
         
           
          <li class="menu-item">
            <a href="myacc" class="menu-link">
              <i class='bx bxs-user-account'></i>
              <div data-i18n="Analytics">My account</div>
            </a>
          </li>
          <li class="menu-item">
            <a href="logout" class="menu-link">
              <i class='bx bx-log-out-circle'></i>
              <div data-i18n="Analytics">Logout</div>
            </a>
          </li>

        

      
         
           
        </ul>
      </aside>
    <!-- Content -->

    <div class="container-xxl">
      <div class="authentication-wrapper authentication-basic container-p-y">
        <div class="authentication-inner">
          <!-- Register Card -->
          <div class="card">
            <div class="card-body">
              <!-- Logo -->
             
              <!-- /Logo -->
              <h4 class="mb-2">Manage account</h4>
            

              <form id="formAuthentication" class="mb-3" action="updateinfo" method="POST">
                <div class="mb-3">
                  <label for="username" class="form-label">Full name </label>
                  <input
                    type="text"
                    class="form-control"
                    id="username"
                    value="<%=a.fullname%>"
                    name="name"
                    placeholder="Enter your Full name"
                    autofocus
                  />
                </div>
                <div class="mb-3">
                    <label for="username" class="form-label">Roll number</label>
                    <input
                      type="text"
                      class="form-control"
                     
                      value="<%=a.rollnumber%>"
                      
                     
                      autofocus readonly
                    />
                  </div>
                <div class="mb-3">
                  <label for="codechefusername" class="form-label">Codechef username</label>
                  <input type="text" value="<%=a.codechefusername%>" class="form-control" title="Copy and paste exact username." id="ccuser" name="ccuser" placeholder="Enter your exact codechef username." />
                </div>
                <div class="mb-3">
                  <label for="passoutyear" class="form-label">Year</label>
                  
                 <select name="pyear" id="pyear" class="form-select form-select-lg"  >
                    
                 </select>
                 <input type="text" id="ip" class="form-control" name="pyear" value="<%=a.passout%>" readonly>
                <a href="#" id="ipc">CHANGE</a>
                </div>
                 <div class="mb-3">
                  <label for="passoutyear" class="form-label">Branch</label>
<!--<select name="branch" id="branch" class="form-select form-select-lg" ></select>    --> 
<input type="text" class="form-control" name="branch" value="<%=a.branch%>" readonly>
           </div>
                <div class="mb-3">
                  <label for="passoutyear" class="form-label">Section</label>
                  <input type="text" class="form-control" name="section"  id="section" value="<%=a.section%>" />
                </div>
                <div class="mb-3 form-password-toggle">
                  <label class="form-label" for="password">New password(Type only if you want to change)</label>
                  <div class="input-group input-group-merge">
                    <input
                      type="password"
                      id="password"
                      class="form-control"
                      name="password"
                      placeholder="&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;"
                      aria-describedby="password"
                    />
                    <span class="input-group-text cursor-pointer"><i class="bx bx-hide"></i></span>
                  </div>
                </div>

                 
           
                <button class="btn btn-primary d-grid w-100">Update</button>
              </form>

              
            </div>
          </div>
          <!-- Register Card -->
        </div>
      </div>
    </div>

    <!-- / Content -->

   

    <!-- Core JS -->
    <!-- build:js assets/vendor/js/core.js -->
    <script src="../assets/vendor/libs/jquery/jquery.js"></script>
    <script src="../assets/vendor/libs/popper/popper.js"></script>
    <script src="../assets/vendor/js/bootstrap.js"></script>
    <script src="../assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>

    <script src="../assets/vendor/js/menu.js"></script>
    <!-- endbuild -->

    <!-- Vendors JS -->

    <!-- Main JS -->
    <script src="../assets/js/main.js"></script>

    <!-- Page JS -->

    <!-- Place this tag in your head or just before your close body tag. -->
    <script async defer src="https://buttons.github.io/buttons.js"></script>
  </body>
  <script>
    $(document).ready(function(){
        $("#pyear").removeAttr("name");
        $("#pyear").hide()
        $("#ipc").click(function (){

            $("#ipc").remove();
            $("#ip").remove();
            $("#pyear").attr("name","pyear");
            $("#pyear").show()
        });
      $.ajax({
            type: "POST",
            url: "/getyears",
             
            
            success: function(result){
console.log(result)
 
var $el = $("#pyear");
 
$.each(JSON.parse(result), function(key,value) {
  $el.append($("<option></option>")
     .attr("value", value.year).text(value.year));
});

}
    });
    $.ajax({
            type: "POST",
            url: "/getdept",
             data:{year:"1ST YEAR"},
            
            success: function(result){
console.log(result)
 
var $el = $("#branch");
$el.empty()
 
$.each(JSON.parse(result), function(key,value) {
  if(value.dept!='0'&&value.dept!="")
 { $el.append($("<option></option>")
     .attr("value", value.dept).text(value.dept));}
});


}
    });
  
    $("#pyear").change(function() {
  if ($(this).data('options') === undefined) {
     $(this).data('options', $('#branch option').clone());
  }
  var id = $(this).val();
  $.ajax({
            type: "POST",
            url: "/getdept",
             data:{year:id},
            
            success: function(result){
console.log(result)
 
var $el = $("#branch");
 $el.empty()
 
$.each(JSON.parse(result), function(key,value) {
  if(value.dept!='0'&&value.dept!="")
 { $el.append($("<option></option>")
     .attr("value", value.dept).text(value.dept));}
});

}
    });
});
  
  
  
  
  
  
  
  });




  </script>
</html>
