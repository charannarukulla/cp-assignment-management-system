  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js"></script>
<script src="https://cdn.datatables.net/plug-ins/1.10.11/sorting/date-eu.js"></script>
  <script>

    $(document).ready(function(){
     $('#allassign').DataTable({
         
        paging: true,
        "autoWidth": true,
        "ordering": true,
"processing":true,
"searching":true,

"ajax": {
    "url": "/get_reportdata",
    "data":{id:<%=id%>},
    "method":"POST",
    "dataSrc": function ( json ) {
      for ( var i=0, ien=json.length ; i<ien ; i++ ) {
        json[i][0] = '<a href="/message/'+json[i][0]+'>View message</a>';
      }
      return json;
    }
  },
 
  dom: 'Bfrtip',
        buttons: [
            'copy', 'csv', 'excel', 'pdf', 'print'
        ]
    ,
    paging:true,dom: 'Blfrtip', responsive: true,
    buttons: [

        {
            extend: 'pdfHtml5',
         className:"btn btn-primary",
          
                filename:"Assignment id <%=id%>"
             ,title:"Assignment id <%=id%>"
        },
        {
            extend:'csvHtml5',  className:"btn btn-primary",
            filename:"Assignment id <%=id%>"
             ,title:"Assignment id <%=id%>"
        },
        {
            extend:'excelHtml5',  className:"btn btn-primary",
            filename:"Assignment id <%=id%>"
             ,title:"Assignment id <%=id%>"
        },
        {
            extend:'print',
            filename:"Assignment id <%=id%>"
             ,title:"Assignment id <%=id%>"
        }
    ],
  "order": [ 6, 'desc' ],
        "columns":[
            {
                "data":"fullname"
            },
            {
                "data":"rollnumber",
                 
               
            },
            {
                "data":"passout",
                render:(d)=>{return d+" passout"}
            },
            {
                "data":"branch"
            },
            {
                "data":"section"
            },
            {
                "data":"score"
            },
            {
                "data":"score",
                render:(score)=>{return (score/<%=totalcodes%>)*100;}
            }
        ]
       
   
     });
    })
    </script>
